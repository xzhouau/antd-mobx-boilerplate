{
  "name": "kino",
  "version": "1.0.0",
  "description": "Project Kino",
  "main": "index.jsx",
  "author": "jzhou",
  "license": "MIT",
  "scripts": {
    "test": "mocha config/testSetup.js --require ignore-styles \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./config/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "clean": "rimraf dist/*",
    "lint": "esw webpack.config.* src config",
    "lint:watch": "npm run lint -- --watch",
    "styleguide": "styleguidist server --config config/styleguide.config.js",
    "styleguide:build": "styleguidist build --config config/styleguide.config.js",
    "open:dist": "babel-node config/distServer.js",
    "start": "npm-run-all --parallel test:watch dev lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node config/mock.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js  --color --progress --hot",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --config config/webpack.config.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "antd": "^3.5.3",
    "axios": "^0.18.0",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "prop-types": "^15.6.1",
    "rc-drawer-menu": "^1.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.24.4",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "isparta": "^4.0.0",
    "json-server": "^0.13.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^7.1.2",
    "lodash": "^4.17.10",
    "lodash-decorators": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "offline-plugin": "^5.0.3",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-hot-loader": "^4.2.0",
    "react-loadable": "^5.4.0",
    "react-styleguidist": "^7.0.17",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.10",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "sideEffects": false,
  "engines": {
    "node": ">8.11.0"
  }
}
