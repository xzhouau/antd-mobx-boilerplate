{
  "presets": [
    [
      "latest",
      {
        "loose": true,
        "modules": false
      }
    ],
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "react-hot-loader/babel",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "test": {
      "plugins": [["babel-plugin-webpack-alias", { "config": "./config/webpack.config.js" }]]
    }
  }
}
